{% extends 'base.html.twig' %}

{% block title %}liste de users {% endblock %}

{% block body %}
<h1> liste des users </h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">username</th>
      <th scope="col">Email</th>
     
      <th scope="col">password</th>
    </tr>
  </thead>
  <tbody>
{% for user in users %}
    <tr> <th scope="row"
      <td>{{user.Username}}</td>
      <td>{{user.Email}}</td>
      
      <td>{{user.Password}}</td>
      <td><a href="{{path('detail',{'id':user.id})}}" classe="btn-primary">detail </a> </td>
    </tr>

{% endfor %}
  </tbody>

</table>

{% endblock %}
